<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="hoathis.demo.faker" class="Faker\Generator" factory-class="Faker\Factory" factory-method="create">
            <argument>fr_FR</argument>
        </service>

        <service id="hoathis.dome.websocket.echoback" class="Hoathis\DemoBundle\WebSocket\Module\Chat">
            <tag name="hoa.websocket.module"/>
        </service>

        <service id="hoathis.dome.websocket.command" class="Hoathis\DemoBundle\WebSocket\Module\Faker">
            <tag name="hoa.websocket.module"/>
        </service>
    </services>

</container>
